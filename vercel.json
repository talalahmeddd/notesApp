{
    "version": 2,
    "builds": [{
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
            "pythonVersion": "3.9"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "api/app.py"
    }],
    "env": {
        "FLASK_ENV": "production"
    }
}